{
  "parser": "babel-eslint",
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "settings": {
    "react": {
      "version": "detect",
      "pragma": "React",
      "createClass": "createReactClass"
    }
  },
  "env": {
    "browser": true,
  },
  "globals": {
    "process": false,
    "module": false,
    "it": false,
    "expect": false,
    "require": false,
    "__dirname": false,
    "projectPathGlobal": false,
    "toolPathGlobal": false,
    "global": false,
  },
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1,
    "react/prop-types": 1,
  }
}
